<script>
    domready=false;
    document.addEventListener("DOMContentLoaded", function(){
        domready = true;
    });
    function load_asset(url, event_name, callback) {
        var urls = Array.isArray(url)?url:[url];
        urls = urls.filter(function(e){return e});
        if(urls.length>0) {
            var load_event = false;
            if (event_name && event_name.length > 0) {
                // load_event = new Event(event_name,{bubbles:!0,cancelable:!0});
                // создание события для IE11-
                load_event = document.createEvent('Event');
                load_event.initEvent(event_name, true, true);
            }


            callback = (callback && {}.toString.call(callback) === '[object Function]')?callback:false;
            urls.map(function (url, idx, array) {
                var ext = url.split('.').pop();
                if(ext=='js') {
                    var new_asset = document.createElement('script');
                    new_asset.type = 'text/javascript';
                    new_asset.src = url;
                    new_asset.defer = 'defer';
                } else if(ext=='css') {
                    var new_asset = document.createElement('link');
                    new_asset.rel = 'stylesheet';
                    new_asset.href = url;
                }
                if(new_asset) {
                    new_asset.async = false;
                    if (new_asset.readyState) {
                        new_asset.onreadystatechange = function() {
                            if (new_asset.readyState == "loaded" || new_asset.readyState == "complete") {
                                new_asset.onreadystatechange = null;
                                if (idx == array.length - 1) {
                                    if(callback) callback(load_event);
                                    if(load_event) {
                                        var domloadtimer = setInterval(function () {
                                            if (domready) {
                                                document.dispatchEvent(load_event);
                                                clearInterval(domloadtimer);
                                            }
                                        }, 10);
                                    }
                                }
                            }
                        };
                    } else {
                        new_asset.onload = function() {
                            if (idx == array.length - 1) {
                                if(callback) callback(load_event);

                                if(load_event) {
                                    var domloadtimer = setInterval(function () {
                                        if (domready) {
                                            document.dispatchEvent(load_event);
                                            clearInterval(domloadtimer);
                                        }
                                    }, 10);
                                }
                            }
                        };
                    }
                    document.getElementsByTagName("body")[0].appendChild(new_asset);
                }
            });}
    }
    // Runs boolfn function 1<=b times (every t milliseconds), if boolfn returns true runs callback function
    try_call = function(boolfn,callback){
        var attemt=1,
            t=25,
            b=10;
        var check = setInterval(function() {
            attemt++;
            if(boolfn()){
                clearInterval(check);
                callback(boolfn());

            } else if(attemt>b){
                clearInterval(boolfn());
            }
        },t);
    };

    var scripts_list = [
        '/ii/lib/jquery.validate.min.js','/ig/js/xslider.js','/ii/js/common.js','/ig/js/classes/popup.js','/ii/js/classes/hiddenbox.js','/ii/js/jivosite.js','/ig/js/components/scroll_to_top.js'
    ];

    var styles_list = [
        '/in/css/general/helper/helper.min.css'
    ];

        load_asset(scripts_list, 'scriptsready');
    load_asset(styles_list);

</script>

