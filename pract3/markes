<script>
    document.addEventListener('DOMContentLoaded', function (){
        var changeVersionButtons = Array.prototype.slice.call(document.querySelectorAll('.switch_site_version'));

        if(changeVersionButtons){
            changeVersionButtons.forEach(function (changeVersionButton) {
                changeVersionButton.onclick = function (){

                    var targetSiteVersion = 'm';
                    var cookie = getModeCookie();

                    var cookieParams = '; expires=Fri, 31 Dec 2035 23:59:59 GMT; Domain=.chitai-gorod.ru; Path=/';

                    if(!cookie){
                        document.cookie = 'uamod=' + encodeURIComponent(targetSiteVersion) + cookieParams;
                    } else {
                        document.cookie = document.cookie.replace(/(?:(?:^|.*;\s*)uamod\s*\=\s*([^;]*).*$)|^.*$/, 'uamod=' + targetSiteVersion + cookieParams);
                    }

                    window.location.replace('https://m.chitai-gorod.ru' + getRouteAdditional());
                };
            });
        }

        function getModeCookie(){
            return document.cookie.replace(/(?:(?:^|.*;\s*)uamod\s*\=\s*([^;]*).*$)|^.*$/, "$1");
        }

        function getRouteAdditional(){
            var additionalPart = '';

            if(window.location.pathname){
                additionalPart += window.location.pathname;
            }

            if(window.location.search){
                additionalPart += window.location.search;
            }

            if(window.location.hash){
                additionalPart += window.location.hash;
            }

            return additionalPart;
        }
    })
